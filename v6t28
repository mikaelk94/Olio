#include <iostream>

using namespace std;

class muoto {
private:

protected:
    double w,h;

public:
};

class suorakulmio : public muoto {
private:

public:
    void set_values(double a, double b) {
       w=a;
       h=b;
    }

    int area () {
        return w*h;
    }
};

class nelio : public suorakulmio {
private:
public:
};

int main()
{
    nelio s;
    s.set_values(5,4);
    cout << s.area() << endl;
    return 0;
}
